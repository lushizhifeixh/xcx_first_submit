/*cart_store_goods  */

.cart_store_goods {
  position: relative;
  height: 242rpx;
}

.cart_store_goods:before{
  width: 710rpx;
  height: 2rpx;
  position: absolute;
  display: block;
  bottom: 0;
  content: '';
  background: #f0f1f1;
  left: -34rpx;
}
.seller_nameNum:before {
  width: 710rpx;
  height: 2rpx;
  position: absolute;
  display: block;
  bottom: 0;
  content: '';
  background: #f0f1f1;
  left: -34rpx;
}
