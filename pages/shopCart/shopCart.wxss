@import "../lessPage/searchCompResult/searchCompResult.wxss"
.page_container,
.hasCart_style {
  height: 100%;
  overflow:hidden;
}

.cart_all_Num {
  width: 100%;
  height: 88rpx;
  background: #fff;
  line-height: 88rpx;
}
.empty_cart_text{
  position:absolute;
top:6rpx;

}
.cart_goods_detail{
  width:370rpx;
}
/*--顶部购物车总量 cart_all_detail  */

.shopping_cart_num {
  width: 350rpx;
  text-align: center;
}

.size_box {
  width: 200rpx;
  text-align: center;
}

/*中间购物车详情  */

.cart_all_detail {
  flex: 1;
  background: #f0f1f1;
  height: 920rpx;
  /* position:fixed;
  top:88rpx; */
}

.all_detail_box {
  padding-bottom: 210rpx;
  box-sizing: border-box;
}

.cart_detail_markName {
  height: 80rpx;
  min-width: 125rpx;
  line-height: 80rpx;
  text-align: center;
  display: inline-block;
  margin-left: calc(50% - 125rpx);
  position: relative;
}
.absolte_bottom_price{
  position:absolute;
right:0;
bottom:0;

}
.cart_detail_markName:before {
  width: 28rpx;
  height: 8rpx;
  content: '';
  display: block;
  position: absolute;
  left: -40rpx;
  top: 37rpx;
  background: #37c65c;
}

.cart_detail_markName:after {
  width: 28rpx;
  height: 8rpx;
  position: absolute;
  display: block;
  right: -40rpx;
  top: 37rpx;
  content: '';
  background: #37c65c;
}
.property_goods_del text{
  width:100rpx;
  height:48rpx;
  line-height:48rpx;
border:1rpx solid #FF6328;
border-radius:8rpx;
color:#FF6328;
float:right;

}
.seller_number_detail {
  width: 710rpx;
  min-height: 319rpx;
  background: #fff;
  box-shadow: 0 0 0 0 #f0f1f1;
  border-radius: 4px 4px 0 0;
  margin: 0 auto;
  padding-left: 34rpx;
  padding-right: 28rpx;
  box-sizing: border-box;
}

.seller_nameNum {
  width: 100%;
  height: 80rpx;
  padding: 20rpx 0rpx 0 0;
  box-sizing: border-box;
}

.store_name_only {
  width: 278rpx;
}

.total_price_01 {
  width: 274rpx;
  text-align: right;
}

.total_icon_contain {
  height: 81rpx;
  width: 45rpx;
}

.cart_select {
  width: 32rpx;
  height: 32rpx;
}
.lossEfficacy{
  width:68rpx;
	height:32rpx;
	background: #99A9BF;
  text-align: center;
  line-height: 32rpx;
	border-radius: 100px;
  letter-spacing: 0.29px;
  margin-left:-10rpx;

}
.cart_list_numRepat {
  width: 170rpx;
  height: 48rpx;
  border: 1rpx solid #c0ccda;
  border-radius: 4rpx;
  text-align: center;
  overflow-y: hidden;
}

.cart_list_numRepat input {
  width: 74rpx;
  height: 48rpx;
  border-left: 1rpx solid #c0ccda;
  border-right: 1rpx solid #c0ccda;
  border-radius: 4rpx;
}

.list_minus, .list_add {
  width: 48rpx;
  height: 48rpx;
  font-size: 37rpx;
  line-height: 48rpx;
  border: 0 solid #c0ccda;
  border-radius: 4rpx 0 0 4rpx;
}

