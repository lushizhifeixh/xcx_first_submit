/*筛选市场点击事件*/
.animation{
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: rgba(0, 0, 0, 0.6);
  overflow: hidden;  
}
.animation-content{
  width:566rpx;
  height:100%;
  position:fixed;
  top:0;
  z-index:1001;
  background:#FAFAFA;
  border-radius:3px;
  overflow-y:scroll;
  overflow-x: hidden;
  right:0;
  box-sizing: border-box;
  padding:70rpx 30rpx 20rpx 30rpx;
}
@keyframes goLeft{
  from{
    right:-566rpx;
  }
  to{
    right:0;
  }
}

.goToLeft{
  animation: goLeft 0.3s ease-out;
}
